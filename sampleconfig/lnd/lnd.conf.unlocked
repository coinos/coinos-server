externalip=lnd:9735
alias=coinos
color=#FF3200
tlsextradomain=lnd
listen=0.0.0.0:9735
rpclisten=0.0.0.0:10009
restlisten=0.0.0.0:8080
accept-amp=1
accept-keysend=1

# uncomment this after lnd wallet is initialized
wallet-unlock-password-file=/root/.lnd/pwd 

[Bitcoin]
bitcoin.active=1
